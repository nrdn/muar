.architect_header
	margin-top 10px
	font-size 24px

.architects_all
	float left
	display block
	text-decoration none
	margin-right 15px
	&:hover
		color red
	&:after
		content '>'
		color black
		font-size 21px
		margin-left 15px

.content_block
	margin-top 10px
	overflow hidden

.architect_block
	margin-bottom 10px

.architect_block_text
	display inline

.architect_block_text:before
	content 'Ôºç'

.architect_block_title
	font-family ma_unitwmedium
	font-size 18px
	text-decoration none
	&:hover
		color #f00
.architect_attached_objects
	width 1300px

.architect_object
	float left
	width 240px
	height 200px
	display block
	position relative
	margin-right 20px
	background-size cover
	background-position 50%
	background-repeat no-repeat
	margin-bottom 20px

.architect_object:hover
	color #f00

.architect_object_name
	bottom 0
	width 100%
	background #fff
	position absolute

.architect_attached_title
	font-size 22px
	line-height 22px
	border-bottom 1px solid #000
	text-align center
	margin-bottom 10px
	padding 3px 0
