.architect_title
	width 1280px
	position relative
	margin 10px auto 0
	font-size 21px
	line-height 22px

.content_block
	margin-top 10px
	overflow hidden

.architect_block
	margin-bottom 10px

.architect_block_text
	display inline

.architect_block_text:before
	content 'Ôºç'

.architect_block_title
	font-family ma_unitwmedium
	font-size 18px
	text-decoration none
	&:hover
		color #f00
.architect_attached_objects
	width 1300px

.architect_object
	float left
	width 240px
	height 200px
	display block
	position relative
	margin-right 20px
	background-size cover
	background-position 50%
	background-repeat no-repeat

.architect_object:hover
	color #f00

.architect_object_name
	bottom 0
	width 100%
	background #fff
	position absolute