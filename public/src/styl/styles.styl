.navigate_block
  width 1280px
  height 40px
  margin 10px auto
  background-color white
  z-index 999999
  font-size 14pt

.navigate_styles_block
  overflow hidden

.navigate_style_block
  float left

.navigate_style_title
  cursor pointer
  margin-right 10px

.navigate_style_progress
  margin-top 3px
  height 2px
  background-color red
  width 0%

.navigate_style_ages
  display none
  position absolute
  margin-top 5px

.navigate_age
  float left
  cursor pointer
  margin-right 10px

.styles_block
  white-space nowrap
  overflow hidden
  width 1280px
  position absolute
  left 50%
  margin-left -640px
  bottom 0
  top 110px

.styles_inner, .style_block
  display inline-block
  height: 100%

.style_block
  overflow auto
  height 100%

.style_block_inner
  overflow hidden

.age_block
  width 1280px
  overflow hidden
  border-top 1px solid black

.age_title
  text-align center
  padding-bottom 20px

.object_block
  width 240px
  display block
  float left
  text-decoration none
  margin-right 20px
  margin-bottom 20px
  &:nth-child(5n)
    margin-right 0px

.object_image
  width 100%
  height 160px
  background-size cover
  background-position center