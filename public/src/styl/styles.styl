.navigate_block
  width 1280px
  min-height 40px
  margin 0 auto
  background-color white
  z-index 999999

.navigate_styles_block
  overflow hidden

.navigate_style_block
  float left
  margin-right 10px

.navigate_style_title
  cursor pointer

.navigate_style_ages
  display none
  position absolute

.navigate_age
  float left
  cursor pointer
  margin-right 10px

.outer
  white-space nowrap
  overflow hidden
  width 1280px
  position absolute
  left 50%
  margin-left -640px
  bottom 0
  top 80px

.age
  width 1280px
  overflow hidden
  border-top 1px solid black

.age_title
  text-align center
  padding-bottom 20px

.object
  width 240px
  height 180px
  background-color grey
  float left
  margin-right 20px
  margin-bottom 20px
  &:nth-child(5n)
    margin-right 0px

.inner, .style
  display inline-block

.style
  overflow auto
  height 100%