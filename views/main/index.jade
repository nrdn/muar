extends ../layout

block title
	title МУАР

block content_style
	link(rel='stylesheet', type='text/css', href='/build/css/main.css' + version)

block content_scripts
	script(type='text/javascript', src='/build/js/main.js' + version)


block content
	h1 Главная
	each era in eras
		h2= 'Стиль: ' + era.era.title.ru
		each age in era.ages
			h3= 'Подстиль: ' + age.age
			b Список объектов:
			br
			each object in age.objects
				b= object.title.ru + ' (  ' + object.interval.start.getUTCFullYear() + ' - ' + object.interval.end.getUTCFullYear() + ' ) '
				br
				br
